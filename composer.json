{
    "name": "spysystem/canadapost-client",
    "version": "1.0.3.1",
    "description": "Library for Canada Post REST API",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "repositories": [
       {"type": "composer", "url": "https://repo.packagist.com/spysystem/"},
       {"packagist.org": false}
 	],
    "homepage": "http://swagger.io",
    "license": "proprietary",
    "authors": [
        {
            "name": "Swagger and contributors",
            "homepage": "https://github.com/swagger-api/swagger-codegen"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0",
        "pear/xml_serializer": "*",
        "pear/xml_parser": "v1.3.7",
        "spysystem/json-simplexmlelement-decorator": "~1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "CanadaPost\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "CanadaPost\\Test\\" : "test/" }
    }
}
